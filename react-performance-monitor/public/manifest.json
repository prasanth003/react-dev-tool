{
  "manifest_version": 3,
  "name": "React Performance Monitor",
  "version": "1.0.2",
  "description": "React Performance Monitor",
  "devtools_page": "devtools.html",
  "icons": {
    "16": "icons/icon-16.png",
    "48": "icons/icon-48.png",
    "128": "icons/icon-128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "http://localhost/*",
        "http://127.0.0.1/*"
      ],
      "js": [
        "content.js"
      ],
      "run_at": "document_end"
    },
    {
      "matches": [
        "http://localhost/*",
        "http://127.0.0.1/*"
      ],
      "js": [
        "main-world.js"
      ],
      "run_at": "document_start",
      "world": "MAIN"
    }
  ]
}